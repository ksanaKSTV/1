import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
    System.out.println("Введите три целых числа:");
        Scanner num = new Scanner(System.in);
        int num1 = num.nextInt();
        int num2 = num.nextInt();
        int num3 = num.nextInt();

        if (num3 < (num1*num2) && (num3%num1==0 || num3%num2==0)) {
            System.out.print("Можно отломить часть состоящую ровно из " + num3 + " долек");
       }
        else {
            System.out.print("Нельзя отломить часть состоящую ровно из " + num3 + " долек");
        }

    }
}
